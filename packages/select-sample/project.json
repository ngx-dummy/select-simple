{
	"projectType": "application",
	"root": "packages/select-sample",
	"sourceRoot": "packages/select-sample/src",
	"prefix": "ngx-dummy",
	"targets": {
		"build": {
			"executor": "@nrwl/angular:webpack-browser",
			"outputs": ["{options.outputPath}"],
			"options": {
				"outputPath": "dist/packages/select-sample",
				"index": "packages/select-sample/src/index.html",
				"main": "packages/select-sample/src/main.ts",
				"polyfills": "packages/select-sample/src/polyfills.ts",
				"tsConfig": "packages/select-sample/tsconfig.app.json",
				"inlineStyleLanguage": "scss",
				"assets": ["packages/select-sample/src/favicon.ico", "packages/select-sample/src/assets"],
				"styles": ["packages/select-sample/src/styles.scss"],
				"scripts": [],
				"customWebpackConfig": {
					"path": "packages/select-sample/webpack.config.js"
				}
			},
			"configurations": {
				"production": {
					"budgets": [
						{
							"type": "initial",
							"maximumWarning": "500kb",
							"maximumError": "1mb"
						},
						{
							"type": "anyComponentStyle",
							"maximumWarning": "2kb",
							"maximumError": "4kb"
						}
					],
					"fileReplacements": [
						{
							"replace": "packages/select-sample/src/environments/environment.ts",
							"with": "packages/select-sample/src/environments/environment.prod.ts"
						}
					],
					"outputHashing": "all",
					"customWebpackConfig": {
						"path": "packages/select-sample/webpack.prod.config.js"
					}
				},
				"development": {
					"buildOptimizer": false,
					"optimization": false,
					"vendorChunk": true,
					"extractLicenses": false,
					"sourceMap": true,
					"namedChunks": true
				}
			},
			"defaultConfiguration": "production"
		},
		"serve": {
			"executor": "@nrwl/angular:webpack-server",
			"configurations": {
				"production": {
					"browserTarget": "select-sample:build:production"
				},
				"development": {
					"browserTarget": "select-sample:build:development"
				}
			},
			"defaultConfiguration": "development",
			"options": {
				"port": 8088
			}
		},
		"extract-i18n": {
			"executor": "@angular-devkit/build-angular:extract-i18n",
			"options": {
				"browserTarget": "select-sample:build"
			}
		},
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"options": {
				"lintFilePatterns": ["packages/select-sample/src/**/*.ts", "packages/select-sample/src/**/*.html"]
			}
		},
		"test": {
			"executor": "@nrwl/jest:jest",
			"outputs": ["coverage/packages/select-sample"],
			"options": {
				"jestConfig": "packages/select-sample/jest.config.js",
				"passWithNoTests": true
			}
		},
		"serve-mfe": {
			"executor": "@nrwl/workspace:run-commands",
			"options": {
				"commands": ["nx serve select-sample"]
			}
		}
	},
	"tags": ["app", ""]
}
